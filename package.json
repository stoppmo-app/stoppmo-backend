{
    "name": "stoppmo-backend",
    "version": "1.0.0",
    "description": "Main backend for StopPMO IOS App.",
    "scripts": {
      "prepare": "npm run install:all",
      "prepare:action": "npm install",
      "generate": "",
      "format": "swiftformat ./",
      "format:lint": "swiftformat --lint ./",
      "lint": "swiftlint ./",
      "build:all": "npm-run-all --parallel build:backend",
      "build:backend": "swift build",
      "install:swiftlint": "brew install swiftlint",
      "install:swiftformat": "brew install swiftformat",
      "install:all": "npm-run-all --sequential install:swiftlint install:swiftformat",
      "install:quality-check-tools": "npm-run-all --sequential install:swiftlint install:swiftformat",
      "quality-checks:all": "npm-run-all --sequential format:lint lint"
    },
    "keywords": [],
    "author": "williamFernsV3",
    "license": "ISC",
    "devDependencies": {
      "npm-run-all": "^4.1.5"
    }
  }
